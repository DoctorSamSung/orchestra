{
  "name": "forward-interop",
  "main": "lib/service.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/start-service",
    "build": "babel src --out-dir lib",
    "build-msg": "mkdir -p lib && pbjs -t static-module -w commonjs --keep-case -o lib/messages.js src/messages/*.proto"
  },
  "dependencies": {
    "async": "^2.6.0",
    "common-nodejs": "file:src/common",
    "express": "^4.16.2",
    "koa": "^2.5.1",
    "koa-protobuf": "^0.1.0",
    "koa-router": "^7.4.0",
    "protobufjs": "~6.8.6",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-preset-env": "^1.6.1"
  },
  "babel": {
    "sourceMaps": "inline",
    "plugins": [
      "add-module-exports",
      "source-map-support"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
